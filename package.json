{
	"name": "cr-reminder",
	"version": "0.0.1",
	"type": "module",
	"description": "This is an example of an Apify Actor.",
	"engines": {
		"node": ">=18.0.0"
	},
	"dependencies": {
		"apify": "^3.4.2",
		"crawlee": "^3.13.8"
	},
	"devDependencies": {
		"@apify/eslint-config": "^1.0.0",
		"@apify/tsconfig": "^0.1.1",
		"@types/node": "^22.15.32",
		"@types/testing-library__jest-dom": "^6.0.0",
		"eslint": "^9.29.0",
		"eslint-config-prettier": "^10.1.5",
		"globals": "^16.2.0",
		"prettier": "^3.5.3",
		"vitest": "^2.1.4",
		"tsx": "^4.20.3",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.34.1"
	},
	"scripts": {
		"start": "npm run start:dev",
		"start:prod": "node dist/main.js",
		"start:dev": "set APIFY_MEMORY_MBYTES=0 && tsx src/main.ts",
		"build": "tsc",
		"lint": "eslint",
		"lint:fix": "eslint --fix",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"test": "vitest run",
		"test:watch": "vitest"
	},
	"author": "It's not you it's me",
